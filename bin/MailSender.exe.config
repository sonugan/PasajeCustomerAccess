<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 --></configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--<add key="FromEmail" value="novedades.ar@prudential.com" />-->
    <add key="FromEmail" value="prudentialpea@gmail.com" />
    <!--casilla desde la que se enviaran los mails-->
    <add key="HtmlEmail" value="true" />
    <!--si el template del mail tiene formato html-->
    <add key="AppName" value="CustomerAccess" />
    <!--<add key="Mail-ServiceBaseUrl" value="http://ar01f141vd/WSMail/api/Emails/SendEmail" />-->
    <add key="Mail-ServiceBaseUrl" value="http://sofrelab-iis1.cloudapp.net:97/api/sendEmail/send/" />
    <!--servicio de envio de mails-->
    <add key="SymmetricKey" value="CustomerAccessDataKey" />
    <!-- Clave simetrica de desencriptacion -->
    <add key="Certificate" value="SSISCertificate" />
    <!-- Certificado de desencriptacion -->
    <add key="EmailType" value="9" />
    <add key="NumberRetry" value="3" />
    <add key="TestClientEmail" value="gnp.gaston@gmail.com" />
    <add key="HeaderImage" value="http://www.prudentialseguros.com.ar/CentralPrud/uploads/archivos/ai5OHMu.png" />
    <add key="FooterImage" value="http://www.prudentialseguros.com.ar/CentralPrud/uploads/archivos/GNWtp8V.png" />
    <add key="StartDate" value="21/03/2018"/><!--formato dd/MM/yyyy-->
    <add key="SendAfterDays" value="7"/>
  </appSettings>
  <connectionStrings>
    <add name="CustomerAccessEntities" providerName="System.Data.SqlClient" connectionString="Data Source=sofrelab-sql1.cloudapp.net;Initial Catalog=CustomerAccessQa;User ID=customeraccess;Password=customeraccess" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>
